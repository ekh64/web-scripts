{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["cjs/**", "esm/**", "types/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": [],
      "inputs": [
        "src/**/*.jsx",
        "src/**/*.js",
        "test/**/*.jsx",
        "test/**/*.js",
        "src/**/*.tsx",
        "src/**/*.ts",
        "test/**/*.ts",
        "test/**/*.tsx"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
